<h1>Student Form</h1>
<%= form_tag students_path do%>
    <label for="student_first_name">What is the Students first name?:</label><br>
        <%= text_field_tag :'student[first_name]'%> <br>
    <label for="student_last_name">What is the Students last name?:</label> <br>
        <%= text_field_tag :'student[last_name]'%> <br>
    <%= submit_tag "Submit Student"%>
<% end %>